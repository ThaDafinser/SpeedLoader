{
    "name":              "thadafinser/speed-loader",
    "description":       "Load classes faster",
    "type":              "library",
    "license":           "MIT",
    "homepage":          "https://github.com/ThaDafinser/SpeedLoader",
    "keywords":          [
        "Speed",
        "classmap",
        "performance"
    ],
    "authors": [
        {
            "name":     "Martin Keckeis",
            "email":    "martin.keckeis1@gmail.com"
        }
    ],
    "require": {
        "php":                     ">=5.5",
        "zendframework/zend-code": ">2.3.0,<3.0"
    },
    "require-dev": {
        "phpunit/phpunit":             "~4.0",
        "fabpot/php-cs-fixer": "~1.5"
    },
    "autoload": {
        "psr-4": {
            "SpeedLoader\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SpeedLoaderTest\\": "tests/SpeedLoaderTest",
            "SpeedLoaderTestAsset\\": "tests/SpeedLoaderTestAsset"
        }
    }
}
